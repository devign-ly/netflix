{
  "project_name": "netflix",
  "include": ["src", ".prettierrc.json"],
  "exclude": ["__tests__", "node_modules"],
  "variables": ["src/tokens.css"],
  "web_server_url": "http://127.0.0.1:3000",
  "run": "DISABLE_ESLINT_PLUGIN=true yarn start",
  "bypass_proxy": true,
  "main_branch": "main",
  "meta_dir": "src/.mightymeld",
  "view_dir": "src/previews",
  "run_prettier": true,
  "generative_enabled": true,
  "prefab_dir": "src/prefab"
}
